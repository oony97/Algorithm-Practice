    "# 프로그래머스\n",
    "## 크기가 작은 부분문자열"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(t, p):\n",
    "    count = 0\n",
    "    for i in range(len(t)-len(p) + 1):\n",
    "        if int(t[i:i+len(p)]) <= int(p):\n",
    "            count += 1\n",
    "    return count"
   ]
  }
